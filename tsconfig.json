{
  "compileOnSave": false,
  "compilerOptions": {
    "outDir": ".",
    "sourceMap": true,
    "allowSyntheticDefaultImports": true,
    "noImplicitAny": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "declaration": true,
    "declarationDir": "./types",
    "declarationMap": false,
    "moduleResolution": "node",
    "module": "esnext",
    "target": "es5",
    "baseUrl": "./",
    "experimentalDecorators": true,
    "lib": [
      "DOM",
      "es2016"
    ],
    // "types": [
    //   "vue",
    //   "vue-router",
    //   "wuhao-network"
    // ]
  },
  "include": [
    "src/**/*.ts",
  ],
  "exclude": [
    "node_modules",
  ]
}